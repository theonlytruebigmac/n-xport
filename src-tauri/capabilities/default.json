{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "opener:allow-open-path",
    "dialog:default",
    "fs:default",
    "fs:allow-write",
    "fs:allow-read",
    "fs:allow-exists",
    "updater:default",
    "process:allow-restart"
  ]
}